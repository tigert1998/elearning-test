{
  "manifest_version": 3,
  "name": "elearning test",
  "description": "苏电e学堂考试插件",
  "version": "5.6",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ],
      "css": [
        "styles.css",
        "katex/katex.min.patched.css"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script-injector.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "service-worker.js",
    "mode": "module"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "downloads",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "tiku/*",
        "injected-script.js",
        "katex/fonts/*",
        "*.js.map"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}